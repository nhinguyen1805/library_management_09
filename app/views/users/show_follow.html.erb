<% provide(:title, @title) %>
<div class="row">
  <aside class="col-md-12">
    <section class="user_info">
      <h1><%= @title %></h1>
    </section>
  </aside>
  <div class="col-md-8">
    <% if @users.any? %>
      <ul class="users follow">
        <%= render @users %>
      </ul>
      <%= will_paginate %>
    <% end %>
  </div>
</div>
